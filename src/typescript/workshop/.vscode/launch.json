{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Agent",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npx",
      "runtimeArgs": [
        "--no-deprecation",
        "tsx",
        "--inspect",
        "${workspaceFolder}/src/main.ts"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}
